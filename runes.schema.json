{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "RunesList",
    "type": "array",
    "items": {
        "type": "object",
        "required": [
            "divisibility",
            "id",
            "name",
            "number",
            "spaced_name",
            "symbol"
        ],
        "properties": {
            "id": {
                "type": "string",
                "pattern": "^\\d+:\\d+$"
            },
            "name": {
                "type": "string"
            },
            "spaced_name": {
                "type": "string"
            },
            "number": {
                "type": "integer",
                "minimum": 0
            },
            "divisibility": {
                "type": "integer",
                "minimum": 0
            },
            "symbol": {
                "type": "string"
            }
        },
        "additionalProperties": false
    }
}